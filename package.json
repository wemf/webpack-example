  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wemf/webpack-example/issues"
  },
  "homepage": "https://github.com/wemf/webpack-example#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "style-loader": "0.19.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
  }
